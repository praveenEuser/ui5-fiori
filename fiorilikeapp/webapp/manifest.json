{
    "_version":"1.9.0",
    "sap.app":{
        "id": "sasu",
        "description":"{{APP_DESC}}",
        "applicationVersion": "1.0.0",
        "type": "application",
        "title":"{{APP_TIT}}",
        "i18n": "i18n/i18n.properties"
    },
    "sap.ui":{
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://home"
        },
        "deviceTypes": {
            "desktop": true,
            "phone": true,
            "tablet": true
        }
    },
    "sap.ui5":{
        "dependencies": {
            "minUI5Version": "1.87.0",
            "libs": {
                "sap.m":{},
                "sap.ui.table":{} 
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "library": {
            "i18n":true,
            "css":"css/mystyle.css"
        },
        "models": {
            "i18n":{
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
                "":{
                    "type": "sap.ui.model.json.JSONModel",
                    "uri": "model/mockdata/dataset.json"
                }
            }
    }
}